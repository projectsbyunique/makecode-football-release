/ Find the opponent in the standings, change their record\n    const gameOpponentAbbreviation = Teams.getTeamProperty(opponent, Teams.TeamProperty.TeamAbbreviation) as string\n    const currentTeamAbbreviation = Teams.getTeamProperty(CurrentTeam, Teams.TeamProperty.TeamAbbreviation) as string\n    let standings = blockSettings.readStringArray(\"standings\")\n\n    function rollBasedOnRating(rating: number) {\n        // Rating is 0-5\n        let chance = 0.3 + (rating / 5) * 0.4\n        return Math.random() < chance\n    }\n\n    let foundIndex: number = null\n    for (let n = 0; n < standings.length; n++) {\n        if (standings[n].includes(gameOpponentAbbreviation)) {\n            foundIndex = n\n            let opponentStanding = standings[n]\n            let b = opponentStanding.split(\"/\")[1]\n            let o = opponentStanding.split(\"/\")[2]\n            let u = opponentStanding.split(\"/\")[3]\n\n            if (awayScore == homeScore) {\n                standings[n] = gameOpponentAbbreviation + \"/\" + b + \"/\" + o + \"/\" + (parseInt(u) + 1).toString()\n            }\n\n            if (isAway == 0) {\n                // False, the player not away, they're at home.\n                if (awayScore < homeScore) {\n                    // Player won the game as the home team.\n                    standings[n] = gameOpponentAbbreviation + \"/\" + b + \"/\" + (parseInt(o) + 1).toString() + \"/\" + u\n                } else {\n                    // Player lost the game as the home team.\n                    standings[n] = gameOpponentAbbreviation + \"/\" + (parseInt(b) + 1).toString() + \"/\" + o + \"/\" + u\n                }\n            } else {\n                // True, the player is away.\n                if (awayScore < homeScore) {\n                    // Player lost the game as the away team.\n                    standings[n] = gameOpponentAbbreviation + \"/\" + (parseInt(b) + 1).toString() + \"/\" + o + \"/\" + u\n                } else {\n                    // Player won the game as the away team.\n                    standings[n] = gameOpponentAbbreviation + \"/\" + b + \"/\" + (parseInt(o) + 1).toString() + \"/\" + u\n                }\n            }\n        } else if (standings[n].includes(currentTeamAbbreviation)) {\n            let currentStanding = standings[n]\n            let c = currentStanding.split(\"/\")[1]\n            let p = currentStanding.split(\"/\")[2]\n            let v = currentStanding.split(\"/\")[3]\n\n            if (awayScore == homeScore) {\n                standings[n] = currentStanding + \"/\" + c + \"/\" + p + \"/\" + (parseInt(v) + 1).toString()\n            }\n\n            if (isAway == 0) {\n                // False, the player not away, they're at home.\n                if (awayScore < homeScore) {\n                    // Player won the game as the home team.\n                    standings[n] = currentTeamAbbreviation + \"/\" + (parseInt(c) + 1).toString() + \"/\" + p + \"/\" + v\n                } else {\n                    // Player lost the game as the home team.\n                    standings[n] = currentTeamAbbreviation + \"/\" + c + \"/\" + (parseInt(p) + 1).toString() + \"/\" + v\n                }\n            } else {\n                // True, the player is away.\n                if (awayScore < homeScore) {\n                    // Player lost the game as the away team.\n                    standings[n] = currentTeamAbbreviation + \"/\" + c + \"/\" + (parseInt(p) + 1).toString() + \"/\" + v\n                } else {\n                    // Player won the game as the away team.\n                    standings[n] = currentTeamAbbreviation + \"/\" + (parseInt(c) + 1).toString() + \"/\" + p + \"/\" + v\n                }\n            }\n        } else {\n            let opponentStanding2 = standings[n]\n            let standingAbbreviation = opponentStanding2.split(\"/\")[0]\n            let d = opponentStanding2.split(\"/\")[1]\n            let q = opponentStanding2.split(\"/\")[2]\n            let e = opponentStanding2.split(\"/\")[3]\n\n            const rating = Teams.getTeamProperty(opponent, Teams.TeamProperty.Rating) as number\n            const win = rollBasedOnRating(rating)\n\n            if (Math.randomRange(0, 10) == 10) {\n                // Tie the Game\n                standings[n] = standingAbbreviation + \"/\" + d + \"/\" + q + \"/\" + (parseInt(e) + 1).toString()\n            }\n\n            if (win) {\n                standings[n] = standingAbbreviation + \"/\" + (parseInt(d) + 1).toString() + \"/\" + q + \"/\" + e\n            } else {\n                standings[n] = standingAbbreviation + \"/\" + d + \"/\" + (parseInt(q) + 1).toString() + \"/\" + e\n            }\n        }\n    }\n    blockSettings.writeStringArray(\"standings\", standings)\n}\nif (!blockSettings.exists(\"skipintro\")) {\n    blockSettings.writeBoolean(\"skipintro\", false)\n}\nenableTabsMenu = true\n_quarter = 1\ngameClock = 90\nopponentYardage = 25\ninterceptionOdds = 0.15\nfumbleOdds = 0.15\nwillScore = Math.percentChance(50)\nmodes.addMode(\"preSnap\")\nmodes.addMode(\"onPlay\")\nmodes.addMode(\"quarterEnd\")\nendingplay = false\nCurrentAwayTeam = Teams.getTeamFromEnum(Teams.TeamEnum.ARI)\nCurrentHomeTeam = Teams.getTeamFromEnum(Teams.TeamEnum.LAR)\nCurrentTeam = CurrentHomeTeam\n_play = 0\nplayFormationImagesArray = [assets.image`play1`, assets.image`play2`, assets.image`play3`]\nlerpSpeed = 0.1\nCameraX = 80\nstart_screen(blockSettings.readBoolean(\"skipintro\"))\nconsole.log(\"on start has been returned too\")\ngame.onUpdate(function () {\n    if (Score) {\n        fancyText.setText(Score, \"\" + Teams.getTeamProperty(CurrentAwayTeam, Teams.TeamProperty.TeamAbbreviation) + \" \" + AwayScore + \"-\" + HomeScore + \" \" + Teams.getTeamProperty(CurrentHomeTeam, Teams.TeamProperty.TeamAbbreviation))\n    }\n    if (qtrClock) {\n        fancyText.setText(qtrClock, secondsToMs(gameClock))\n        if (gameClock < 60 && gameClock > 10) {\n            fancyText.setColor(qtrClock, 5)\n        } else if (gameClock < 11) {\n            fancyText.setColor(qtrClock, 2)\n        } else {\n            fancyText.setColor(qtrClock, 1)\n        }\n    }\n    if (qtrText) {\n        fancyText.setText(qtrText, \"Q\" + _quarter)\n    }\n})\ngame.onUpdate(function () {\n    if (!(onField)) {\n        for (let value122 of sprites.allOfKind(SpriteKind.NonPlayerPlayers)) {\n            value122.setFlag(SpriteFlag.Invisible, true)\n        }\n    } else {\n        for (let value13 of sprites.allOfKind(SpriteKind.NonPlayerPlayers)) {\n            value13.setFlag(SpriteFlag.Invisible, false)\n        }\n    }\n})\ngame.onUpdate(function () {\n    if (sprites.allOfKind(SpriteKind.Field).length > 0) {\n        lerpCameraX(CameraX)\n    } else {\n        scene.centerCameraAt(80, 60)\n    }\n})\ngame.onUpdate(function () {\n\n})\ngame.onUpdate(function () {\n\n})\ngame.onUpdateInterval(1, function () {\n    set_offensive_colors()\n    set_defensive_colors()\n})\ngame.onUpdateInterval(1, function () {\n\n})\nforever(function () {\n\n})\nforever(function () {\n\n})\n","README.md":" \n\n\n> Open this page at [https://projectsbyunique.github.io/football/](https://projectsbyunique.github.io/football/)\n\n## Use as Extension\n\nThis repository can be added as an **extension** in MakeCode.\n\n* open [https://arcade.makecode.com/](https://arcade.makecode.com/)\n* click on **New Project**\n* click on **Extensions** under the gearwheel menu\n* search for **https://github.com/projectsbyunique/football** and import\n\n## Edit this project\n\nTo edit this repository in MakeCode.\n\n* open [https://arcade.makecode.com/](https://arcade.makecode.com/)\n* click on **Import** then click on **Import URL**\n* paste **https://github.com/projectsbyunique/football** and click import\n\n#### Metadata (used for search, rendering)\n\n* for PXT/arcade\n<script src=\"https://makecode.com/gh-pages-embed.js\"></script><script>makeCodeRender(\"{{ site.ma